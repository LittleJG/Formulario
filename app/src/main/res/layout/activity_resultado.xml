<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".Resultado">

    <TextView
        android:id="@+id/emailResul"
        android:layout_width="249dp"
        android:layout_height="58dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/dataDeNascResul"
        app:layout_constraintTop_toBottomOf="@+id/nomeResul"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/nomeResul"
        android:layout_width="249dp"
        android:layout_height="57dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/emailResul"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/dataDeNascResul"
        android:layout_width="249dp"
        android:layout_height="53dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/telefResul"
        app:layout_constraintTop_toBottomOf="@+id/emailResul"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/telefResul"
        android:layout_width="241dp"
        android:layout_height="57dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/mensResul"
        app:layout_constraintTop_toBottomOf="@+id/dataDeNascResul"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/mensResul"
        android:layout_width="241dp"
        android:layout_height="78dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="25dp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/telefResul"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="240dp"
        android:layout_height="67dp"
        android:layout_marginTop="25dp"
        android:layout_marginBottom="100dp"
        android:text="Senha  * * * * * "
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mensResul"
        tools:layout_editor_absoluteX="95dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Nome:"
        app:layout_constraintBottom_toTopOf="@+id/nomeResul"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="95dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="Email:"
        app:layout_constraintBottom_toTopOf="@+id/emailResul"
        app:layout_constraintTop_toBottomOf="@+id/nomeResul"
        tools:layout_editor_absoluteX="94dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Data de Nascimento:"
        app:layout_constraintBottom_toTopOf="@+id/dataDeNascResul"
        app:layout_constraintTop_toBottomOf="@+id/emailResul"
        tools:layout_editor_absoluteX="95dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:text="Telefone:"
        app:layout_constraintBottom_toTopOf="@+id/telefResul"
        app:layout_constraintTop_toBottomOf="@+id/dataDeNascResul"
        tools:layout_editor_absoluteX="95dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="Breve Apresentação:"
        app:layout_constraintBottom_toTopOf="@+id/mensResul"
        app:layout_constraintTop_toBottomOf="@+id/telefResul"
        tools:layout_editor_absoluteX="95dp" />

</androidx.constraintlayout.widget.ConstraintLayout>